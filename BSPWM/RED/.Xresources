! clean up urxvt, add keybinds
URxvt.intensityStyles: false
URxvt.scrollBar: false

! URxvt.background: #282828
! XTerm*background: #272822

! font
URxvt*font: xft:DroidSansMono Nerd Font:size=9:antialias=true:hinting=true

! DroidSansMono Nerd Font
URxvt.letterSpace: -1

! terminal padding
URxvt.internalBorder: 15 
! URxvt*shading: 30
! transparent terminal
! URxvt.transparent:	true
! URxvt.shading:		15

Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! clickable links, clipboard support, mouse wheel support, and on the fly font resizing
! URxvt.perl-ext-common: default,matcher,clipboard,vtwheel,resize-font
! URxvt.url-launcher: /usr/bin/chromium
! URxvt.matcher.button: 1
! URxvt.matcher.rend.0: Uline Bold fg6

! X colors.
! Generated by 'wal'
emacs*foreground:   #807A86
emacs*background:   #1D1C25
URxvt*foreground:   #807A86
XTerm*foreground:   #807A86
UXTerm*foreground:  #807A86
URxvt*background:   [95]#1D1C25
XTerm*background:   #1D1C25
UXTerm*background:  #1D1C25
URxvt*cursorColor:  #807A86
XTerm*cursorColor:  #807A86
UXTerm*cursorColor: #807A86
URxvt*borderColor:  [95]#1D1C25

! Colors 0-15.
*.color0: #1D1C25
*color0:  #1D1C25
*.color1: #605F67
*color1:  #605F67
*.color2: #923D3D
*color2:  #923D3D
*.color3: #8D494A
*color3:  #8D494A
*.color4: #A75253
*color4:  #A75253
*.color5: #D55252
*color5:  #D55252
*.color6: #F14545
*color6:  #F14545
*.color7: #807A86
*color7:  #807A86
*.color8: #77767c
*color8:  #77767c
*.color9: #605F67
*color9:  #605F67
*.color10: #923D3D
*color10:  #923D3D
*.color11: #8D494A
*color11:  #8D494A
*.color12: #A75253
*color12:  #A75253
*.color13: #D55252
*color13:  #D55252
*.color14: #F14545
*color14:  #F14545
*.color15: #807A86
*color15:  #807A86

! Black color that will not be affected by bold highlighting.
*.color66: #1D1C25
*color66:  #1D1C25

! Rofi colors.
rofi.color-window: #1D1C25, #923D3D, #923D3D
rofi.color-normal: #1D1C25, #807A86, #1D1C25, #923D3D, #1D1C25
rofi.color-active: #1D1C25, #807A86, #1D1C25, #923D3D, #1D1C25
rofi.color-urgent: #1D1C25, #605F67, #1D1C25, #605F67, #807A86

! Xclock colors.
XClock*foreground: #807A86
XClock*background: #1D1C25
XClock*majorColor:  rgba:80/7a/86/ff
XClock*minorColor:  rgba:80/7a/86/ff
XClock*hourColor:   rgba:80/7a/86/ff
XClock*minuteColor: rgba:80/7a/86/ff
XClock*secondColor: rgba:80/7a/86/ff

! Set depth to make transparency work.
URxvt*depth: 32

